{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "schedule:daily",
    "group:allNonMajor",
    ":semanticCommits",
    ":semanticCommitTypeAll(chore)",
    ":disableDependencyDashboard"
  ],
  "labels": ["deps", "renovate"],
  "baseBranches": ["main"],
  "rangeStrategy": "bump"
}
